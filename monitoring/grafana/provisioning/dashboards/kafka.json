{
  "title": "Kafka Messaging",
  "uid": "kafka-dashboard",
  "schemaVersion": 36,
  "style": "dark",
  "tags": [
    "aviation",
    "kafka",
    "messaging",
    "flights"
  ],
  "refresh": "10s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "panels": [
    {
      "type": "row",
      "title": "Message Flow Overview",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "stat",
      "title": "Messages Produced/sec",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 1
      },
      "targets": [
        {
          "expr": "sum(rate(flights_kafka_messages_produced_total[5m]))",
          "legendFormat": "Produced/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "red",
                "value": 100
              }
            ]
          },
          "unit": "reqps"
        }
      }
    },
    {
      "type": "stat",
      "title": "Messages Sent/sec",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 1
      },
      "targets": [
        {
          "expr": "sum(rate(flights_kafka_messages_sent_total[5m]))",
          "legendFormat": "Sent/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "red",
                "value": 100
              }
            ]
          },
          "unit": "reqps"
        }
      }
    },
    {
      "type": "stat",
      "title": "Error Rate",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 1
      },
      "targets": [
        {
          "expr": "sum(rate(flights_kafka_messages_errors_total[5m]))",
          "legendFormat": "Errors/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          },
          "unit": "reqps"
        }
      }
    },
    {
      "type": "stat",
      "title": "Success Rate %",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 1
      },
      "targets": [
        {
          "expr": "100 * sum(rate(flights_kafka_messages_sent_total[5m])) / sum(rate(flights_kafka_messages_produced_total[5m]))",
          "legendFormat": "Success %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 95
              },
              {
                "color": "green",
                "value": 99
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "type": "row",
      "title": "Message Throughput",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 5
      }
    },
    {
      "type": "timeseries",
      "title": "Kafka Message Rates",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "targets": [
        {
          "expr": "sum(rate(flights_kafka_messages_produced_total[5m])) by (topic, event_type)",
          "legendFormat": "Produced - {{topic}}/{{event_type}}"
        },
        {
          "expr": "sum(rate(flights_kafka_messages_sent_total[5m])) by (topic, event_type)",
          "legendFormat": "Sent - {{topic}}/{{event_type}}"
        },
        {
          "expr": "sum(rate(flights_kafka_messages_errors_total[5m])) by (topic, error_type)",
          "legendFormat": "Errors - {{topic}}/{{error_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "pointSize": 5,
            "fillOpacity": 10
          },
          "unit": "reqps"
        }
      }
    },
    {
      "type": "piechart",
      "title": "Message Distribution by Topic",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 6
      },
      "targets": [
        {
          "expr": "sum(rate(flights_kafka_messages_sent_total[5m])) by (topic)",
          "legendFormat": "{{topic}}"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        },
        "pieType": "pie",
        "displayLabels": [
          "name",
          "percent"
        ]
      }
    },
    {
      "type": "row",
      "title": "Performance Metrics",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 14
      }
    },
    {
      "type": "timeseries",
      "title": "Kafka Producer Latency",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 15
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(flights_kafka_producer_latency_seconds_bucket[5m])) by (le, topic, event_type))",
          "legendFormat": "P50 - {{topic}}/{{event_type}}"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(flights_kafka_producer_latency_seconds_bucket[5m])) by (le, topic, event_type))",
          "legendFormat": "P95 - {{topic}}/{{event_type}}"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(flights_kafka_producer_latency_seconds_bucket[5m])) by (le, topic, event_type))",
          "legendFormat": "P99 - {{topic}}/{{event_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "pointSize": 5,
            "fillOpacity": 10
          },
          "unit": "s"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Avro Serialization Time",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 15
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(flights_kafka_serialization_duration_seconds_bucket[5m])) by (le, topic, event_type))",
          "legendFormat": "P50 - {{topic}}/{{event_type}}"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(flights_kafka_serialization_duration_seconds_bucket[5m])) by (le, topic, event_type))",
          "legendFormat": "P95 - {{topic}}/{{event_type}}"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(flights_kafka_serialization_duration_seconds_bucket[5m])) by (le, topic, event_type))",
          "legendFormat": "P99 - {{topic}}/{{event_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "pointSize": 5,
            "fillOpacity": 10
          },
          "unit": "s"
        }
      }
    },
    {
      "type": "row",
      "title": "Error Analysis",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 23
      }
    },
    {
      "type": "timeseries",
      "title": "Error Breakdown by Type",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "targets": [
        {
          "expr": "sum(rate(flights_kafka_messages_errors_total[5m])) by (error_type)",
          "legendFormat": "{{error_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "pointSize": 5,
            "fillOpacity": 20,
            "stacking": {
              "mode": "normal"
            }
          },
          "unit": "reqps"
        }
      }
    },
    {
      "type": "table",
      "title": "Error Summary",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      },
      "targets": [
        {
          "expr": "sum(rate(flights_kafka_messages_errors_total[5m])) by (topic, error_type)",
          "legendFormat": "",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "job": true,
              "instance": true
            },
            "renameByName": {
              "Value": "Error Rate (req/s)",
              "topic": "Topic",
              "error_type": "Error Type"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "displayMode": "list",
            "inspect": false
          },
          "unit": "reqps"
        }
      }
    },
    {
      "type": "row",
      "title": "Kafka Infrastructure Metrics",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 32
      }
    },
    {
      "type": "timeseries",
      "title": "Partition Distribution",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 33
      },
      "targets": [
        {
          "expr": "sum(rate(flights_kafka_messages_sent_total[5m])) by (partition)",
          "legendFormat": "Partition {{partition}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "pointSize": 5,
            "fillOpacity": 10
          },
          "unit": "reqps"
        }
      }
    },
    {
      "type": "stat",
      "title": "Active Topics",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 33
      },
      "targets": [
        {
          "expr": "count(group by (topic) (flights_kafka_messages_produced_total))",
          "legendFormat": "Topics"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "type": "stat",
      "title": "Active Partitions",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 33
      },
      "targets": [
        {
          "expr": "count(group by (partition) (flights_kafka_messages_sent_total))",
          "legendFormat": "Partitions"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "short"
        }
      }
    }
  ]
}